<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>HelloServlets</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>


	<servlet>
		<servlet-name>DirServ</servlet-name>
		<servlet-class>com.revature.directservlet.DirectServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DirServ</servlet-name>
		<url-pattern>/dirserv</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>IndirServ</servlet-name>
		<servlet-class>com.revature.indirectservlet.IndirectServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IndirServ</servlet-name>
		<url-pattern>/indirserv</url-pattern>
		<!--  Can you have more than 1 url pattern? YES -->
		<url-pattern>/testing</url-pattern>
	</servlet-mapping>






	<!-- This is an example of an autogenerated servlet -->
	<servlet>
		<description></description>
		<display-name>MyDemoServlet</display-name>
		<servlet-name>MyDemoServlet</servlet-name>
		<servlet-class>com.revature.indirectservlet.MyDemoServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyDemoServlet</servlet-name>
		<url-pattern>/MyDemoServlet</url-pattern>
	</servlet-mapping>
</web-app>